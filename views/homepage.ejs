<!DOCTYPE html>
<html lang="en">

<body>
  <%- include('partials/index') %>
    <div class="main">

    <img class="book" src="/images/myresources.png">


    <div class="feature" >


      <table class="table">
        <thead>
          <tr class="table-head">
            <th scope="col">Title</th>
            <th scope="col">Link</th>
            <th scope="col">Description</th>


          </tr>
        </thead>
        <tbody>
          <% for(let resource in resources) { %>
            <tr class="table-row">
              <td><a href="/resource/<%=resources[resource].title %>"><%= resources[resource].title %></a></td>
              <td><%= resources[resource].url %></td>
              <td><%= resources[resource].description %></td>
              <td><%= resources[resource].category %></td>

              <td><%= resources[resource].user %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
  </div>

    <img class="likes" src="/images/likes.png">
    <div class="my-likes">
    <table class="liked-content">
      <thead>
        <tr class ="likes-head"></tr>
          <th scope="col">Title</th>
          <th scope="col">Link</th>
          <th scope="col">Description</th>
          <th scope="col">Added by</th>
        </tr>
      </thead>
      <tbody>
        <% for(let likedResource in likedResources) { %>
          <tr class="like-row">
            <td><%= likedResources[likedResource].title %></td>
            <td><%= likedResources[likedResource].url %></td>
            <td><%= likedResources[likedResource].description %></td>
            <td><%= likedResources[likedResource].category %></td>
            <td><%= likedResources[likedResource].user %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
  </body>
</html>
