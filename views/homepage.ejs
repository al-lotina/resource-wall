<!DOCTYPE html>
<html lang="en">

<body>
  <%- include('partials/index') %>
    <div class="main">

    <img class="book" src="/images/myresources.png">


    <div class="feature" >


      <table class="table">
        <thead>
          <tr class="table-head">
            <th scope="col">Title</th>

            <th scope="col">Added by</th>

          </tr>
        </thead>
        <tbody>
          <% for(let resource in resources) { %>
            <tr class="table-row">
              <td><%= resources[resource].title %></td>

              <td><%= resources[resource].user %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
  </div>

    <img class="likes" src="/images/likes.png">
    <div class="my-likes">
    <table class="liked-content">
      <thead>
        <tr class ="likes-head"></tr>
        <th scope="col">Title</th>
        <th scope="col">Description</th>
      </tr>
      </thead>
      <tbody>
        <% for(let resource in resources) { %>
          <tr class="like-row">
            <td><%= resources[resource].title %></td>

            <td><%= resources[resource].user %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
  </body>
</html>
